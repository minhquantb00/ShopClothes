{"ast":null,"code":"import Layout from \"@/layout/c-main.vue\";\nimport { Swiper, SwiperSlide } from 'swiper/vue';\nimport { Navigation, Autoplay } from 'swiper/modules';\n\n// Import Swiper styles\nimport 'swiper/css';\nimport 'swiper/css/navigation';\nexport default {\n  components: {\n    Layout,\n    Swiper,\n    SwiperSlide\n  },\n  setup() {\n    return {\n      modules: [Navigation, Autoplay]\n    };\n  }\n};","map":{"version":3,"names":["Layout","Swiper","SwiperSlide","Navigation","Autoplay","components","setup","modules"],"sources":["D:\\ProjectCSharp\\themeforest-7O0hM42R-light-able-admin-dashboard-template\\Template\\Vue\\Admin\\src\\views\\components\\advance\\slider.vue"],"sourcesContent":["<script>\nimport Layout from \"@/layout/c-main.vue\"\nimport { Swiper, SwiperSlide } from 'swiper/vue';\n\nimport { Navigation, Autoplay } from 'swiper/modules';\n\n// Import Swiper styles\nimport 'swiper/css';\nimport 'swiper/css/navigation';\n\n\nexport default {\n    components: {\n        Layout,\n        Swiper,\n        SwiperSlide,\n    },\n    setup() {\n        return {\n            modules: [Navigation, Autoplay],\n        };\n    },\n}\n</script>\n\n<template>\n    <Layout>\n        <BRow>\n            <BCol md=\"10\" xxl=\"9\" class=\"mb-4\">\n                <h2>Slider</h2>\n                <p class=\"text-muted\">tiny-slider works with static content and it works in the browser only. If the HTML is\n                    loaded dynamically.</p>\n                <div>\n                    <a class=\"btn btn-sm btn-light-dark rounded-pill px-2\" role=\"button\" target=\"_blank\"\n                        href=\"http://ganlanyuan.github.io/tiny-slider/demo/\">\n                        <i class=\"ti ti-external-link me-1\"></i>\n                        Reference\n                    </a>\n                </div>\n            </BCol>\n        </BRow>\n\n        <BRow>\n            <BCol md=\"6\">\n                <BCard no-body>\n                    <BCardHeader>\n                        <h5>Slider1</h5>\n                    </BCardHeader>\n                    <BCardBody>\n                        <swiper :navigation=\"true\" :autoplay=\"true\" :modules=\"modules\" :loop=\"true\" class=\"mySwiper\">\n                            <swiper-slide> <img src=\"@/assets/images/slider/img-slide-1.jpg\" alt=\"\"\n                                    class=\"img-fluid\"></swiper-slide>\n                            <swiper-slide><img src=\"@/assets/images/slider/img-slide-2.jpg\" alt=\"\"\n                                    class=\"img-fluid\"></swiper-slide>\n                            <swiper-slide> <img src=\"@/assets/images/slider/img-slide-3.jpg\" alt=\"\"\n                                    class=\"img-fluid\"></swiper-slide>\n                        </swiper>\n                    </BCardBody>\n                </BCard>\n            </BCol>\n            <BCol md=\"6\">\n                <BCard no-body>\n                    <BCardHeader>\n                        <h5>Vertical</h5>\n                    </BCardHeader>\n                    <BCardBody>\n                        <swiper :navigation=\"true\" :autoplay=\"true\" :modules=\"modules\" :loop=\"true\" class=\"mySwiper\">\n                            <swiper-slide> <img src=\"@/assets/images/slider/img-slide-1.jpg\" alt=\"\"\n                                    class=\"img-fluid\"></swiper-slide>\n                            <swiper-slide><img src=\"@/assets/images/slider/img-slide-2.jpg\" alt=\"\"\n                                    class=\"img-fluid\"></swiper-slide>\n                            <swiper-slide> <img src=\"@/assets/images/slider/img-slide-3.jpg\" alt=\"\"\n                                    class=\"img-fluid\"></swiper-slide>\n                        </swiper>\n                        <!-- <div class=\"slider2 owl-carousel owl-theme\">\n                            <div class=\"item\">\n                                <img src=\"@/assets/images/slider/img-slide-1.jpg\" alt=\"\" class=\"img-fluid\">\n                            </div>\n                            <div class=\"item\">\n                                <img src=\"@/assets/images/slider/img-slide-2.jpg\" alt=\"\" class=\"img-fluid\">\n                            </div>\n                            <div class=\"item\">\n                                <img src=\"@/assets/images/slider/img-slide-3.jpg\" alt=\"\" class=\"img-fluid\">\n                            </div>\n                        </div> -->\n                    </BCardBody>\n                </BCard>\n            </BCol>\n            <BCol md=\"12\">\n                <BCard no-body>\n                    <BCardHeader>\n                        <h5>Customize</h5>\n                    </BCardHeader>\n                    <BCardBody>\n                        <div id=\"slider3\">\n                            <swiper :slidesPerView=\"3\" :autoplay=\"true\" :spaceBetween=\"30\" :navigation=\"true\"\n                                :modules=\"modules\" :loop=\"true\" class=\"mySwiper\">\n                                <swiper-slide> <img src=\"@/assets/images/slider/img-slide-1.jpg\" alt=\"\"\n                                        class=\"img-fluid\"></swiper-slide>\n                                <swiper-slide><img src=\"@/assets/images/slider/img-slide-2.jpg\" alt=\"\"\n                                        class=\"img-fluid\"></swiper-slide>\n                                <swiper-slide> <img src=\"@/assets/images/slider/img-slide-3.jpg\" alt=\"\"\n                                        class=\"img-fluid\"></swiper-slide>\n                            </swiper>\n                            <!-- <div class=\"customize\" id=\"customize\">\n                                <div>\n                                    <div>\n                                        <img src=\"@/assets/images/slider/img-slide-1.jpg\" alt=\"\" class=\"img-fluid\">\n                                    </div>\n                                </div>\n                                <div>\n                                    <div>\n                                        <img src=\"@/assets/images/slider/img-slide-2.jpg\" alt=\"\" class=\"img-fluid\">\n                                    </div>\n                                </div>\n                                <div>\n                                    <div>\n                                        <img src=\"@/assets/images/slider/img-slide-3.jpg\" alt=\"\" class=\"img-fluid\">\n                                    </div>\n                                </div>\n                                <div>\n                                    <div>\n                                        <img src=\"@/assets/images/slider/img-slide-4.jpg\" alt=\"\" class=\"img-fluid\">\n                                    </div>\n                                </div>\n                            </div> -->\n                            <div class=\"customize-tools\">\n                                <ul class=\"thumbnails\" id=\"customize-thumbnails\">\n                                    <li>\n                                        <img src=\"@/assets/images/slider/img-slide-1.jpg\" alt=\"\">\n                                    </li>\n                                    <li>\n                                        <img src=\"@/assets/images/slider/img-slide-2.jpg\" alt=\"\">\n                                    </li>\n                                    <li>\n                                        <img src=\"@/assets/images/slider/img-slide-3.jpg\" alt=\"\">\n                                    </li>\n                                    <li>\n                                        <img src=\"@/assets/images/slider/img-slide-4.jpg\" alt=\"\">\n                                    </li>\n                                </ul>\n                                <ul class=\"controls\" id=\"customize-controls\">\n                                    <li class=\"prev\">\n                                        <i class=\"feather icon-chevron-left\"></i>\n                                    </li>\n                                    <li class=\"next\">\n                                        <i class=\"feather icon-chevron-right\"></i>\n                                    </li>\n                                </ul>\n                                <div class=\"playbutton-wrapper\">\n                                    <button id=\"customize-toggle\">Pause</button>\n                                </div>\n                            </div>\n                        </div>\n                    </BCardBody>\n                </BCard>\n            </BCol>\n        </BRow>\n    </Layout>\n</template>"],"mappings":"AACA,OAAOA,MAAK,MAAO,qBAAoB;AACvC,SAASC,MAAM,EAAEC,WAAU,QAAS,YAAY;AAEhD,SAASC,UAAU,EAAEC,QAAO,QAAS,gBAAgB;;AAErD;AACA,OAAO,YAAY;AACnB,OAAO,uBAAuB;AAG9B,eAAe;EACXC,UAAU,EAAE;IACRL,MAAM;IACNC,MAAM;IACNC;EACJ,CAAC;EACDI,KAAKA,CAAA,EAAG;IACJ,OAAO;MACHC,OAAO,EAAE,CAACJ,UAAU,EAAEC,QAAQ;IAClC,CAAC;EACL;AACJ","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}