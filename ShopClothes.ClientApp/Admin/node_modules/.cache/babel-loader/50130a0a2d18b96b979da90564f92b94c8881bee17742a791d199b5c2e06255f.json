{"ast":null,"code":"import Layout from \"@/layout/main.vue\";\nimport pageheader from \"@/components/page-header.vue\";\nimport { basicData } from \"./data-basic\";\nexport default {\n  name: \"EXPORT\",\n  data() {\n    return {\n      basicData\n    };\n  },\n  components: {\n    Layout,\n    pageheader\n  }\n};","map":{"version":3,"names":["Layout","pageheader","basicData","name","data","components"],"sources":["D:\\ProjectCSharp\\ShopClothes.Api\\ShopClothes.Api\\ShopClothes.ClientApp\\Admin\\src\\views\\live-preview\\table\\vanila\\export.vue"],"sourcesContent":["<script>\nimport Layout from \"@/layout/main.vue\"\nimport pageheader from \"@/components/page-header.vue\"\nimport { basicData } from \"./data-basic\"\n\nexport default {\n    name: \"EXPORT\",\n    data() {\n        return {\n            basicData\n        }\n    },\n    components: {\n        Layout, pageheader\n    },\n}\n</script>\n\n<template>\n    <Layout>\n        <pageheader title=\"Fetch API\" pageTitle=\"Table\" />\n        <BRow>\n            <div class=\"col-xl-12\">\n                <div class=\"card\">\n                    <div class=\"card-header\">\n                        <h5>Basic Table</h5>\n                        <span class=\"d-block m-t-5\">use class <code>table</code> inside table element</span>\n                    </div>\n                    <div class=\"card-body table-border-style\">\n                        <button class=\"btn btn-light-primary mb-1 btn-sm csv\">Export CSV</button>\n                        <button class=\"btn btn-light-primary mb-1 btn-sm sql ms-1\">Export SQL</button>\n                        <button class=\"btn btn-light-primary mb-1 btn-sm txt ms-1\">Export TXT</button>\n                        <button class=\"btn btn-light-primary mb-1 btn-sm json ms-1\">Export JSON</button>\n                        <div class=\"table-responsive\">\n                            <table class=\"table\" id=\"pc-dt-export\">\n                                <thead>\n                                    <tr>\n                                        <th>Name</th>\n                                        <th>Ext.</th>\n                                        <th>City</th>\n                                        <th data-type=\"date\" data-format=\"YYYY/DD/MM\">Start Date</th>\n                                        <th>Completion</th>\n                                    </tr>\n                                </thead>\n                                <tbody>\n                                    <tr v-for=\"item in basicData\" :key=\"item.id\">\n                                        <td>{{ item.name }}</td>\n                                        <td>{{ item.id }}</td>\n                                        <td>{{ item.location }}</td>\n                                        <td>{{ item.birthDate }}</td>\n                                        <td>{{ item.percentage }}</td>\n                                    </tr>\n                                </tbody>\n                            </table>\n                        </div>\n                    </div>\n                </div>\n            </div>\n        </BRow>\n    </Layout>\n</template>"],"mappings":"AACA,OAAOA,MAAK,MAAO,mBAAkB;AACrC,OAAOC,UAAS,MAAO,8BAA6B;AACpD,SAASC,SAAQ,QAAS,cAAa;AAEvC,eAAe;EACXC,IAAI,EAAE,QAAQ;EACdC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHF;IACJ;EACJ,CAAC;EACDG,UAAU,EAAE;IACRL,MAAM;IAAEC;EACZ;AACJ","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}