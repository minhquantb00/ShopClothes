{"ast":null,"code":"import FooterComponents from \"@/components/footer.vue\";\nimport Navbar from \"@/components/navbar.vue\";\nimport Rightbar from \"@/components/right-bar.vue\";\nimport MenuComponents from \"@/components/sidebar.vue\";\nexport default {\n  name: \"VERTICAL\",\n  components: {\n    Navbar,\n    Rightbar,\n    FooterComponents,\n    MenuComponents\n  },\n  methods: {\n    handleOutsideClick(event) {\n      if (this.$store.state.isMobileSidebarActive && !this.$el.contains(event.target)) {\n        // Close the mobile sidebar\n        this.$store.commit('toggleMobileSidebar');\n      }\n    }\n  },\n  computed: {\n    isFixedWidth() {\n      return this.$store.getters.isFixedWidth;\n    }\n  }\n};","map":{"version":3,"names":["FooterComponents","Navbar","Rightbar","MenuComponents","name","components","methods","handleOutsideClick","event","$store","state","isMobileSidebarActive","$el","contains","target","commit","computed","isFixedWidth","getters"],"sources":["D:\\ProjectCSharp\\themeforest-7O0hM42R-light-able-admin-dashboard-template\\Template\\Vue\\Admin\\src\\layout\\vertical.vue"],"sourcesContent":["<script>\nimport FooterComponents from \"@/components/footer.vue\"\nimport Navbar from \"@/components/navbar.vue\"\nimport Rightbar from \"@/components/right-bar.vue\"\nimport MenuComponents from \"@/components/sidebar.vue\"\n\nexport default {\n    name: \"VERTICAL\",\n    components: {\n        Navbar, Rightbar, FooterComponents, MenuComponents\n    },\n    methods: {\n        handleOutsideClick(event) {\n            if (\n                this.$store.state.isMobileSidebarActive &&\n                !this.$el.contains(event.target)\n            ) {\n                // Close the mobile sidebar\n                this.$store.commit('toggleMobileSidebar');\n            }\n        },\n    },\n    computed: {\n        isFixedWidth() {\n            return this.$store.getters.isFixedWidth;\n        },\n    },\n   \n}\n</script>\n\n<template>\n    <div class=\