{"ast":null,"code":"import Layout from \"@/layout/main.vue\";\nimport pageheader from \"@/components/page-header.vue\";\nimport VueCropper from \"vue-cropperjs\";\nimport \"cropperjs/dist/cropper.css\";\nimport img11 from \"@/assets/images/light-box/l1.jpg\";\nexport default {\n  name: \"IMAGE-CROPER\",\n  components: {\n    Layout,\n    pageheader,\n    VueCropper\n  },\n  data() {\n    return {\n      img11\n    };\n  },\n  methods: {\n    zoom(percent) {\n      this.$refs.cropper.relativeZoom(percent);\n    },\n    rotate(deg) {\n      this.$refs.cropper.rotate(deg);\n    },\n    move(offsetX, offsetY) {\n      this.$refs.cropper.move(offsetX, offsetY);\n    },\n    flipX() {\n      const element = document.querySelector(\".flip-x-btn\");\n      if (element) {\n        let scale = element.getAttribute(\"data-scale\");\n        if (scale) {\n          scale = scale ? -scale : -1;\n          this.$refs.cropper.scaleX(scale);\n          element.setAttribute(\"data-scale\", scale);\n        }\n      }\n    },\n    flipY() {\n      const element = document.querySelector(\".flip-x-btn\");\n      if (element) {\n        let scale = element.getAttribute(\"data-scale\");\n        scale = scale ? -scale : -1;\n        this.$refs.cropper.scaleY(scale);\n        element.setAttribute(\"data-scale\", scale);\n      }\n    },\n    reset() {\n      this.$refs.cropper.reset();\n    }\n  }\n};","map":{"version":3,"names":["Layout","pageheader","VueCropper","img11","name","components","data","methods","zoom","percent","$refs","cropper","relativeZoom","rotate","deg","move","offsetX","offsetY","flipX","element","document","querySelector","scale","getAttribute","scaleX","setAttribute","flipY","scaleY","reset"],"sources":["D:\\ProjectCSharp\\themeforest-7O0hM42R-light-able-admin-dashboard-template\\Template\\Vue\\Admin\\src\\views\\live-preview\\form\\image-croper.vue"],"sourcesContent":["<script>\nimport Layout from \"@/layout/main.vue\"\nimport pageheader from \"@/components/page-header.vue\"\nimport VueCropper from \"vue-cropperjs\";\nimport \"cropperjs/dist/cropper.css\";\n\nimport img11 from \"@/assets/images/light-box/l1.jpg\"\n\nexport default {\n    name: \"IMAGE-CROPER\",\n    components: {\n        Layout, pageheader, VueCropper\n    },\n    data() {\n        return {\n            img11\n        }\n    },\n    methods: {\n        zoom(percent) {\n            this.$refs.cropper.relativeZoom(percent);\n        },\n        rotate(deg) {\n            this.$refs.cropper.rotate(deg);\n        },\n        move(offsetX, offsetY) {\n            this.$refs.cropper.move(offsetX, offsetY);\n        },\n\n        flipX() {\n            const element = document.querySelector(\".flip-x-btn\")\n            if (element) {\n                let scale = element.getAttribute(\"data-scale\")\n                if (scale) {\n                    scale = scale ? -scale : -1;\n                    this.$refs.cropper.scaleX(scale);\n                    element.setAttribute(\"data-scale\", scale);\n                }\n            }\n        },\n        flipY() {\n            const element = document.querySelector(\".flip-x-btn\")\n            if (element) {\n                let scale = element.getAttribute(\"data-scale\");\n                scale = scale ? -scale : -1;\n                this.$refs.cropper.scaleY(scale);\n                element.setAttribute(\"data-scale\", scale);\n\n            }\n        },\n        reset() {\n            this.$refs.cropper.reset();\n        }\n    }\n};\n\n</script>\n\n<template>\n    <Layout>\n        <pageheader title=\"Image Cropper\" pageTitle=\"Forms\" />\n        <BRow>\n            <BCol xl=\"9\">\n                <BCard no-body>\n                    <BCardBody>\n                        <vue-cropper ref=\"cropper\" :src=\"img11\" alt=\"Source Image\" preview=\".preview-lg\" />\n                        <div class=\"mt-2\">\n                            <BLink href=\"javascript:void(0);\" class=\"btn btn-primary\" @click.prevent=\"zoom(0.2)\">Zoom In\n                            </BLink>\n                            <BLink href=\"javascript:void(0);\" class=\"btn btn-primary ms-1\" @click.prevent=\"zoom(-0.2)\">Zoom\n                                Out</BLink>\n                            <BLink href=\"javascript:void(0);\" class=\"btn btn-primary ms-1\" @click.prevent=\"rotate(90)\">\n                                Rotate +90deg</BLink>\n                            <BLink href=\"javascript:void(0);\" class=\"btn btn-primary ms-1\" @click.prevent=\"rotate(-90)\">\n                                Rotate -90deg</BLink>\n                            <BLink href=\"javascript:void(0);\" class=\"btn btn-primary ms-1\" @click.prevent=\"move(-10, 0)\">\n                                Move Left</BLink>\n                            <BLink href=\"javascript:void(0);\" class=\"btn btn-primary ms-1\" @click.prevent=\"move(10, 0)\">Move\n                                Right</BLink>\n                            <BLink href=\"javascript:void(0);\" class=\"btn btn-primary ms-1\"\n                                @click.prevent=\"move(0, -10)\">\n                                Move Up</BLink>\n                            <BLink href=\"javascript:void(0);\" class=\"btn btn-primary ms-1 mt-2\"\n                                @click.prevent=\"move(0, 10)\">Move\n                                Down</BLink>\n                            <BLink href=\"javascript:void(0);\" class=\"btn btn-primary ms-1 flip-x-btn mt-2\" data-scale=\"1\"\n                                @click.prevent=\"flipX\">Flip X</BLink>\n                            <BLink href=\"javascript:void(0);\" class=\"btn btn-primary ms-1 flip-y-btn mt-2\" data-scale=\"1\"\n                                @click.prevent=\"flipY\">Flip Y</BLink>\n                            <BLink href=\"javascript:void(0);\" class=\"btn btn-primary ms-1 mt-2\" @click.prevent=\"reset\">Reset\n                            </BLink>\n                        </div>\n                    </BCardBody>\n                </BCard>\n            </BCol>\n            <BCol xl=\"3\">\n                <div class=\"preview-lg\"></div>\n            </BCol>\n        </BRow>\n    </Layout>\n</template>\n\n<style scoped>\n.preview-lg {\n    width: 100%;\n    height: calc(372px * (9 / 16));\n    overflow: hidden;\n}\n</style>"],"mappings":"AACA,OAAOA,MAAK,MAAO,mBAAkB;AACrC,OAAOC,UAAS,MAAO,8BAA6B;AACpD,OAAOC,UAAS,MAAO,eAAe;AACtC,OAAO,4BAA4B;AAEnC,OAAOC,KAAI,MAAO,kCAAiC;AAEnD,eAAe;EACXC,IAAI,EAAE,cAAc;EACpBC,UAAU,EAAE;IACRL,MAAM;IAAEC,UAAU;IAAEC;EACxB,CAAC;EACDI,IAAIA,CAAA,EAAG;IACH,OAAO;MACHH;IACJ;EACJ,CAAC;EACDI,OAAO,EAAE;IACLC,IAAIA,CAACC,OAAO,EAAE;MACV,IAAI,CAACC,KAAK,CAACC,OAAO,CAACC,YAAY,CAACH,OAAO,CAAC;IAC5C,CAAC;IACDI,MAAMA,CAACC,GAAG,EAAE;MACR,IAAI,CAACJ,KAAK,CAACC,OAAO,CAACE,MAAM,CAACC,GAAG,CAAC;IAClC,CAAC;IACDC,IAAIA,CAACC,OAAO,EAAEC,OAAO,EAAE;MACnB,IAAI,CAACP,KAAK,CAACC,OAAO,CAACI,IAAI,CAACC,OAAO,EAAEC,OAAO,CAAC;IAC7C,CAAC;IAEDC,KAAKA,CAAA,EAAG;MACJ,MAAMC,OAAM,GAAIC,QAAQ,CAACC,aAAa,CAAC,aAAa;MACpD,IAAIF,OAAO,EAAE;QACT,IAAIG,KAAI,GAAIH,OAAO,CAACI,YAAY,CAAC,YAAY;QAC7C,IAAID,KAAK,EAAE;UACPA,KAAI,GAAIA,KAAI,GAAI,CAACA,KAAI,GAAI,CAAC,CAAC;UAC3B,IAAI,CAACZ,KAAK,CAACC,OAAO,CAACa,MAAM,CAACF,KAAK,CAAC;UAChCH,OAAO,CAACM,YAAY,CAAC,YAAY,EAAEH,KAAK,CAAC;QAC7C;MACJ;IACJ,CAAC;IACDI,KAAKA,CAAA,EAAG;MACJ,MAAMP,OAAM,GAAIC,QAAQ,CAACC,aAAa,CAAC,aAAa;MACpD,IAAIF,OAAO,EAAE;QACT,IAAIG,KAAI,GAAIH,OAAO,CAACI,YAAY,CAAC,YAAY,CAAC;QAC9CD,KAAI,GAAIA,KAAI,GAAI,CAACA,KAAI,GAAI,CAAC,CAAC;QAC3B,IAAI,CAACZ,KAAK,CAACC,OAAO,CAACgB,MAAM,CAACL,KAAK,CAAC;QAChCH,OAAO,CAACM,YAAY,CAAC,YAAY,EAAEH,KAAK,CAAC;MAE7C;IACJ,CAAC;IACDM,KAAKA,CAAA,EAAG;MACJ,IAAI,CAAClB,KAAK,CAACC,OAAO,CAACiB,KAAK,CAAC,CAAC;IAC9B;EACJ;AACJ,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}